generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
//membuat tabel

model Siswa{
  id Int @id @default(autoincrement())
  name String 
  age Int
  address String
  birth DateTime
  sex String
  no_parent Int
} 
